<launch>
  <!-- No namespace here as we will share this description. 
       Access with slash at the beginning -->
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description_lab)/urdf/turtlebot3_$(arg model).urdf.xacro" />

  
  <!-- BEGIN ROBOT 1-->
  <group ns="robot1">	
    <param name="tf_prefix" value="robot1_tf" />
    <include file="$(find turtlebot3_gazebo_lab)/launch/robot1.launch" >
      <arg name="init_pose" value="-x  -y -2 -z 0" />
      <arg name="robot_name"  value="Robot1" />
    </include>
  </group>


  <!-- BEGIN ROBOT 2-->
  <!--<group ns="robot2">
    <param name="tf_prefix" value="robot2_tf" />
    <include file="$(find turtlebot3_gazebo_lab)/launch/robot1.launch" >
      <arg name="init_pose" value="-x -1 -y 1 -z 0" />
      <arg name="robot_name"  value="Robot2" />
    </include>
  </group>-->
</launch>
